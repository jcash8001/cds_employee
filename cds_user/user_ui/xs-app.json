{
  "welcomeFile": "/user_ui/index.html",
  "authenticationMethod": "route",
  "logout": {
    "logoutEndpoint": "/do/logout"
  },
  "routes": [
    {
      "source": "^/xsodata/user.xsodata/(.*)$",
      "destination": "user_js_be"
    },
    {
      "source": "^/user_ui/(.*)$",
      "target": "$1",
      "localDir": "webapp"
    }
  ]
}